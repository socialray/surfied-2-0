#: anspress_email.php:134
msgid "New question posted by {asker}"
msgstr "Nouvelle question publiée par {asker}"

#: anspress_email.php:135
msgid ""
"Hello!\r\n"
"A new question is posted by {asker}\r\n"
"\r\n"
"Title: {question_title}\r\n"
"Description:\r\n"
"{question_excerpt}\r\n"
"\r\n"
"Link: {question_link}"
msgstr ""
"Bonjour!\n"
"Une nouvelle question a été publiée par {asker}\n"
"\n"
"Titre : {question_title}\n"
"Description :\n"
"{question_excerpt}\n"
"\n"
"Lien : {question_link}"

